{
  "port": 3000,
  "keep": {
    "lastimage": false,
    "lasthc1": false,
    "lastresult": false
  },
  "ssl": {
    "active": false,
    "key":"",
    "cert":""
  },
  "log": {
    "name": "greenpass2json",
    "graylog": {
      "active": false
    },
  "loglevel": "WARN"
  },
  "metrics": {},
  "security": {
    "allowedMethods":["GET", "OPTIONS", "POST", "HEAD"],
    "onlyDefinedRoutes": true,
    "definedRoutes": [
      "/_health",
      "/_metrics",
      "/_version",
      "/qrcode",
      "/hc1",
      "/last/image",
      "/last/hc1",
      "/last/result"
    ]
  },
  "cors": {
    "allowedHeaders": [
      "Origin",
      "X-Requested-With",
      "Content-Type",
      "Accept"
    ],
    "origin": "*",
    "methods": "GET,HEAD,OPTIONS,POST",
    "preflightContinue": false
  }
}